{
    "name": "valesaude/value-objects",
    "description": "A set of reusable ValueObjects.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "ValeSaude\\ValueObjects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ValeSaude\\ValueObjects\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Vale Sa√∫de Sempre",
            "email": "ti@valesaudesempre.com.br"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^8.83",
        "illuminate/support": "^8.83",
        "spatie/laravel-enum": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2",
        "fakerphp/faker": "^1.20"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "analyse": "phpstan analyze",
        "test": "pest",
        "test:coverage": "pest --coverage"
    }
}
